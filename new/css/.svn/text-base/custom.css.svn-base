.pane-content .node {margin:0; padding:0;}
.peoplesoftcookie {visibility:hidden;}
dl.multiselect dd.a, dl.multiselect dd.a .form-item {width:auto;}
#utilitybox li.leftmost {border-left:none;}
ul.links li {display:block;}
#but, #but1 {width:110px;}
#but a{color:#000000;}
#but1 a{color:#000000;}
#edit-preview {}
#edit-promote {visibility:hidden}
