function hideshowmessage(componentid){
	var content = document.getElementById("otrcontent" + componentid);
	// if currently display
	if (content.style.display == "block"){
		document.images["otrheadingimg" + componentid].src = "http://www.sfsu.edu/template/images/open.png";
		content.style.display = "none";
		
	}//if
	else{
		document.images["otrheadingimg" + componentid].src = "http://www.sfsu.edu/template/images/close.png";
		content.style.display = "block";
	}//else
}